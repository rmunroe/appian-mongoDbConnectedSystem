<appian-plugin name="MongoDB CS Helper" key="com.appiancorp.solutionsconsulting.plugin.mongodb">

    <plugin-info>
        <description>MongoDB CS Helper</description>
        <vendor name="Appian Corporation" url="http://www.appian.com" />
        <version>1.0.5</version>
        <application-version min="19.1" />
    </plugin-info>

    <function-category key="MongoDbCategory" name="MongoDb CS Helper"/>


    <datatype key="MongoDb" name="Mongo DB Data Types">
        <class>com.appiancorp.solutionsconsulting.plugin.mongodb.datatypes.ObjectId</class>
        <class>com.appiancorp.solutionsconsulting.plugin.mongodb.datatypes.Binary</class>
    </datatype>

    <function key="MdbFromJson"
              class="com.appiancorp.solutionsconsulting.plugin.mongodb.expressions.MdbFromJson" />

    <function key="MdbToJson"
              class="com.appiancorp.solutionsconsulting.plugin.mongodb.expressions.MdbToJson" />

</appian-plugin>
