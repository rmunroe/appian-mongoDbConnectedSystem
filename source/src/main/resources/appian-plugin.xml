<appian-plugin name="MongoDB Connected System" key="com.appiancorp.solutionsconsulting.cs.mongodb">

    <plugin-info>
        <description>MongoDB Connected System for Appian</description>
        <vendor name="Appian Solutions Consulting" url="http://www.appian.com"/>
        <version>1.0.0.0</version>
        <application-version min="19.4.0.0"/>
    </plugin-info>

    <connected-system-template key="MongoDbConnectedSystem" name="MongoDbConnectedSystem"
                               class="com.appiancorp.solutionsconsulting.cs.mongodb.MongoDbConnectedSystemTemplate">

        <integration-template key="ListDatabasesIntegrationTemplate" name="ListDatabasesIntegrationTemplate"
                              class="com.appiancorp.solutionsconsulting.cs.mongodb.integrations.ListDatabasesIntegrationTemplate"/>

        <integration-template key="ListCollectionsIntegrationTemplate" name="ListCollectionsIntegrationTemplate"
                              class="com.appiancorp.solutionsconsulting.cs.mongodb.integrations.ListCollectionsIntegrationTemplate"/>

        <integration-template key="ReadFindIntegrationTemplate" name="ReadFindIntegrationTemplate"
                              class="com.appiancorp.solutionsconsulting.cs.mongodb.integrations.ReadFindIntegrationTemplate"/>

        <integration-template key="WriteFindIntegrationTemplate" name="WriteFindIntegrationTemplate"
                              class="com.appiancorp.solutionsconsulting.cs.mongodb.integrations.WriteFindIntegrationTemplate"/>

        <integration-template key="CountIntegrationTemplate" name="CountIntegrationTemplate"
                              class="com.appiancorp.solutionsconsulting.cs.mongodb.integrations.CountIntegrationTemplate"/>

        <integration-template key="ReadAggregateIntegrationTemplate" name="ReadAggregateIntegrationTemplate"
                              class="com.appiancorp.solutionsconsulting.cs.mongodb.integrations.ReadAggregateIntegrationTemplate"/>

        <integration-template key="WriteAggregateIntegrationTemplate" name="WriteAggregateIntegrationTemplate"
                              class="com.appiancorp.solutionsconsulting.cs.mongodb.integrations.WriteAggregateIntegrationTemplate"/>

        <integration-template key="InsertManyIntegrationTemplate" name="InsertManyIntegrationTemplate"
                              class="com.appiancorp.solutionsconsulting.cs.mongodb.integrations.InsertManyIntegrationTemplate"/>

        <integration-template key="InsertOneIntegrationTemplate" name="InsertOneIntegrationTemplate"
                              class="com.appiancorp.solutionsconsulting.cs.mongodb.integrations.InsertOneIntegrationTemplate"/>

        <integration-template key="UpdateManyIntegrationTemplate" name="UpdateManyIntegrationTemplate"
                              class="com.appiancorp.solutionsconsulting.cs.mongodb.integrations.UpdateManyIntegrationTemplate"/>

        <integration-template key="UpdateOneIntegrationTemplate" name="UpdateOneIntegrationTemplate"
                              class="com.appiancorp.solutionsconsulting.cs.mongodb.integrations.UpdateOneIntegrationTemplate"/>

        <integration-template key="ReplaceOneIntegrationTemplate" name="ReplaceOneIntegrationTemplate"
                              class="com.appiancorp.solutionsconsulting.cs.mongodb.integrations.ReplaceOneIntegrationTemplate"/>

        <integration-template key="DeleteManyIntegrationTemplate" name="DeleteManyIntegrationTemplate"
                              class="com.appiancorp.solutionsconsulting.cs.mongodb.integrations.DeleteManyIntegrationTemplate"/>

        <integration-template key="DeleteOneIntegrationTemplate" name="DeleteOneIntegrationTemplate"
                              class="com.appiancorp.solutionsconsulting.cs.mongodb.integrations.DeleteOneIntegrationTemplate"/>

        <integration-template key="DropCollectionIntegrationTemplate" name="DropCollectionIntegrationTemplate"
                              class="com.appiancorp.solutionsconsulting.cs.mongodb.integrations.DropCollectionIntegrationTemplate"/>

    </connected-system-template>
</appian-plugin>
