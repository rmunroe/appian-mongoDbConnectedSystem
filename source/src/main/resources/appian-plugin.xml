<appian-plugin name="MongoDB Connected System" key="com.appiancorp.solutionsconsulting.plugin.mongodb">

    <plugin-info>
        <description>MongoDB Connected System for Appian</description>
        <vendor name="Appian Solutions Consulting" url="http://www.appian.com"/>
        <version>1.0.0.2</version>
        <application-version min="19.4.0.0"/>
    </plugin-info>

    <function-category key="MongoDbCategory" name="MongoDb CS Helper"/>

    <datatype key="MongoDb" name="Mongo DB Data Types">
        <class>com.appiancorp.solutionsconsulting.plugin.mongodb.datatypes.ObjectId</class>
        <class>com.appiancorp.solutionsconsulting.plugin.mongodb.datatypes.Binary</class>
    </datatype>


    <function key="Mdb_Now" class="com.appiancorp.solutionsconsulting.plugin.mongodb.expressions.Mdb_Now"/>
    <function key="Mdb_FromJson" class="com.appiancorp.solutionsconsulting.plugin.mongodb.expressions.Mdb_FromJson"/>
    <function key="Mdb_ToJson" class="com.appiancorp.solutionsconsulting.plugin.mongodb.expressions.Mdb_ToJson"/>

    <function key="Mdb_Field"
              class="com.appiancorp.solutionsconsulting.plugin.mongodb.expressions.operators.Mdb_Field"/>

    <function key="Mdb_Eq"
              class="com.appiancorp.solutionsconsulting.plugin.mongodb.expressions.operators.comparison.Mdb_Eq"/>
    <function key="Mdb_Gt"
              class="com.appiancorp.solutionsconsulting.plugin.mongodb.expressions.operators.comparison.Mdb_Gt"/>
    <function key="Mdb_Gte"
              class="com.appiancorp.solutionsconsulting.plugin.mongodb.expressions.operators.comparison.Mdb_Gte"/>
    <function key="Mdb_In"
              class="com.appiancorp.solutionsconsulting.plugin.mongodb.expressions.operators.comparison.Mdb_In"/>
    <function key="Mdb_Lt"
              class="com.appiancorp.solutionsconsulting.plugin.mongodb.expressions.operators.comparison.Mdb_Lt"/>
    <function key="Mdb_Lte"
              class="com.appiancorp.solutionsconsulting.plugin.mongodb.expressions.operators.comparison.Mdb_Lte"/>
    <function key="Mdb_Ne"
              class="com.appiancorp.solutionsconsulting.plugin.mongodb.expressions.operators.comparison.Mdb_Ne"/>
    <function key="Mdb_Nin"
              class="com.appiancorp.solutionsconsulting.plugin.mongodb.expressions.operators.comparison.Mdb_Nin"/>

    <function key="Mdb_And"
              class="com.appiancorp.solutionsconsulting.plugin.mongodb.expressions.operators.logical.Mdb_And"/>
    <function key="Mdb_Nor"
              class="com.appiancorp.solutionsconsulting.plugin.mongodb.expressions.operators.logical.Mdb_Nor"/>
    <function key="Mdb_Not"
              class="com.appiancorp.solutionsconsulting.plugin.mongodb.expressions.operators.logical.Mdb_Not"/>
    <function key="Mdb_Or"
              class="com.appiancorp.solutionsconsulting.plugin.mongodb.expressions.operators.logical.Mdb_Or"/>


    <connected-system-template key="MongoDbConnectedSystem" name="MongoDbConnectedSystem"
                               class="com.appiancorp.solutionsconsulting.plugin.mongodb.MongoDbConnectedSystemTemplate">

        <integration-template key="ListDatabasesIntegrationTemplate" name="ListDatabasesIntegrationTemplate"
                              class="com.appiancorp.solutionsconsulting.plugin.mongodb.integrations.ListDatabasesIntegrationTemplate"/>

        <integration-template key="ListCollectionsIntegrationTemplate" name="ListCollectionsIntegrationTemplate"
                              class="com.appiancorp.solutionsconsulting.plugin.mongodb.integrations.ListCollectionsIntegrationTemplate"/>

        <integration-template key="ReadFindIntegrationTemplate" name="ReadFindIntegrationTemplate"
                              class="com.appiancorp.solutionsconsulting.plugin.mongodb.integrations.ReadFindIntegrationTemplate"/>

        <integration-template key="WriteFindIntegrationTemplate" name="WriteFindIntegrationTemplate"
                              class="com.appiancorp.solutionsconsulting.plugin.mongodb.integrations.WriteFindIntegrationTemplate"/>

        <integration-template key="CountIntegrationTemplate" name="CountIntegrationTemplate"
                              class="com.appiancorp.solutionsconsulting.plugin.mongodb.integrations.CountIntegrationTemplate"/>

        <integration-template key="ReadAggregateIntegrationTemplate" name="ReadAggregateIntegrationTemplate"
                              class="com.appiancorp.solutionsconsulting.plugin.mongodb.integrations.ReadAggregateIntegrationTemplate"/>

        <integration-template key="WriteAggregateIntegrationTemplate" name="WriteAggregateIntegrationTemplate"
                              class="com.appiancorp.solutionsconsulting.plugin.mongodb.integrations.WriteAggregateIntegrationTemplate"/>

        <integration-template key="InsertManyIntegrationTemplate" name="InsertManyIntegrationTemplate"
                              class="com.appiancorp.solutionsconsulting.plugin.mongodb.integrations.InsertManyIntegrationTemplate"/>

        <integration-template key="InsertOneIntegrationTemplate" name="InsertOneIntegrationTemplate"
                              class="com.appiancorp.solutionsconsulting.plugin.mongodb.integrations.InsertOneIntegrationTemplate"/>

        <integration-template key="UpdateManyIntegrationTemplate" name="UpdateManyIntegrationTemplate"
                              class="com.appiancorp.solutionsconsulting.plugin.mongodb.integrations.UpdateManyIntegrationTemplate"/>

        <integration-template key="UpdateOneIntegrationTemplate" name="UpdateOneIntegrationTemplate"
                              class="com.appiancorp.solutionsconsulting.plugin.mongodb.integrations.UpdateOneIntegrationTemplate"/>

        <integration-template key="ReplaceOneIntegrationTemplate" name="ReplaceOneIntegrationTemplate"
                              class="com.appiancorp.solutionsconsulting.plugin.mongodb.integrations.ReplaceOneIntegrationTemplate"/>

        <integration-template key="DeleteManyIntegrationTemplate" name="DeleteManyIntegrationTemplate"
                              class="com.appiancorp.solutionsconsulting.plugin.mongodb.integrations.DeleteManyIntegrationTemplate"/>

        <integration-template key="DeleteOneIntegrationTemplate" name="DeleteOneIntegrationTemplate"
                              class="com.appiancorp.solutionsconsulting.plugin.mongodb.integrations.DeleteOneIntegrationTemplate"/>

        <integration-template key="CreateCollectionIntegrationTemplate" name="CreateCollectionIntegrationTemplate"
                              class="com.appiancorp.solutionsconsulting.plugin.mongodb.integrations.CreateCollectionIntegrationTemplate"/>

        <integration-template key="DropCollectionIntegrationTemplate" name="DropCollectionIntegrationTemplate"
                              class="com.appiancorp.solutionsconsulting.plugin.mongodb.integrations.DropCollectionIntegrationTemplate"/>

    </connected-system-template>
</appian-plugin>
